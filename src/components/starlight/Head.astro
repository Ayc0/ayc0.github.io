---
import type { Props } from "@astrojs/starlight/props";
import Default from "@astrojs/starlight/components/Head.astro";

import { getImagePath } from '@components/images'


let path: string | undefined = await getImagePath(Astro.props.entry.data.image);
---

<Default {...Astro.props} />

{path ? <meta property="og:image" content={Astro.url.origin + path} /> : null}
{path ? <meta name="twitter:image" content={Astro.url.origin + path} /> : null}
